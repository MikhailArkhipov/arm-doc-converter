{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Converter",
            "type": "node",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--nolazy",
                "-r",
                "ts-node/register/transpile-only"
            ],
            "request": "launch",
            "args": [
                "src/convert.ts",
                "D:\\ARM-doc\\A32",
                "D:\\ARM-doc",
                "A32"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "skipFiles": [
                "<node_internals>/**",
                "node_modules/**"
            ]
        }
    ],
}